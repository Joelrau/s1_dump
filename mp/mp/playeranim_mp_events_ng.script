	//	torso mp_stand_shotgun_fire
	//}
	
// DEFAULT (RIFLES)
	movetype idleprone2idle AND idleprone2idlecr AND idlecr2idleprone
	{
	}
	movetype prone
	{
		torso pt_prone_shoot_auto
